includes:
    - ./vendor/nunomaduro/larastan/extension.neon

parameters:
    level: 8

    paths:
        - app
        - database
        - routes

    excludePaths:
        - storage/*
        - vendor/*
        - public/*

    checkUninitializedProperties: true
    inferPrivatePropertyTypeFromConstructor: true
    reportUnmatchedIgnoredErrors: false
